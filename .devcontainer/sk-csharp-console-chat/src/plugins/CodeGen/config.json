{
    "schema": 1,
    "type": "completion",
    "description": "Generate code from a user prompt in a given language",
    "completion": {
        "max_tokens": 256,
        "temperature": 0,
        "top_p": 0,
        "presence_penalty": 0,
        "frequency_penalty": 0
    },
    "input": {
        "parameters": [
            {
                "name": "input",
                "description": "Input for this semantic function.",
                "defaultValue": "any example of code"
            },
            {
                "name": "grammar",
                "description": "The ANTLR file defining the grammar for the language",
                "defaultValue": "grammars/csharp.g4"
            },
            {
                "name": "history",
                "description": "The chat history to use for context",
                "defaultValue": ""
            }
        ]
    },
    "default_backends": []
}